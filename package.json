{
  "name": "spv-node",
  "version": "0.0.0",
  "description": "Dogecoin SPV wallet (beta)",
  "main": "./bin/main.js",
  "scripts": {
    "start": "DEBUG=* node src/main.js",
    "dev": "DEBUG=*,-nodemon:*,-nodemon nodemon src/main.js",
    "clean": "rm -rf data",
    "test": "NETWORK=testnet ava",
    "generate": "node test/tools/generate.js"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*"
    ]
  },
  "bin": {
    "dogecoin-spv": "./src/bin/main.js"
  },
  "ava": {
    "verbose": true,
    "files": [
      "test/*.test.js"
    ]
  },
  "keywords": [
    "dogecoin",
    "spv",
    "spvnode"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^3.12.1",
    "nodemon": "^1.14.8"
  },
  "dependencies": {
    "bip32": "^2.0.3",
    "bip39": "git+https://github.com/bitcoinjs/bip39.git",
    "bitcoin-merkle-proof": "^0.1.0",
    "bloom-filter": "^0.2.0",
    "bs58": "^4.0.1",
    "bs58check": "^2.1.1",
    "clipboardy": "^2.3.0",
    "debug": "^4.1.1",
    "ecdsa": "^0.7.0",
    "exp-net": "0.0.1",
    "level": "^6.0.0",
    "meow": "^6.1.0",
    "multicast-dns": "^7.0.0",
    "ripemd160": "^2.0.1",
    "secp256k1": "^4.0.2"
  }
}
